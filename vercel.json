{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "src": "/partners$", "dest": "/partners/partners.html" },
    { "src": "/partners/(.*)", "dest": "/partners/$1" },

    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "outputDirectory": "."
}
