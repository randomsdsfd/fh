{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs@v25"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "src": "/partners$", "dest": "/partners/partners.html" },
    { "src": "/partners/(.*)", "dest": "/partners/$1" },

    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "outputDirectory": "."
}
